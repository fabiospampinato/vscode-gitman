{
  "name": "vscode-gitman",
  "displayName": "GitMan",
  "description": "Frontend for GitMan for switching quickly to other repositories.",
  "icon": "resources/logo-128x128.png",
  "version": "1.0.1",
  "license": "MIT",
  "main": "out/extension.js",
  "publisher": "fabiospampinato",
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "gitman.open",
        "title": "GitMan: Open"
      },
      {
        "command": "gitman.openInNewWindow",
        "title": "GitMan: Open in New Window"
      }
    ],
    "keybindings": [
      {
        "command": "gitman.open",
        "key": "ctrl+alt+p",
        "mac": "cmd+alt+p"
      },
      {
        "command": "gitman.openInNewWindow",
        "key": "ctrl+alt+shift+p",
        "mac": "cmd+alt+shift+p"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "rm -rf out && webpack --mode production",
    "publish": "vsce publish && npm publish",
    "compile": "webpack --mode development",
    "compile:watch": "webpack --mode development --watch",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "bugs": {
    "url": "https://github.com/fabiospampinato/vscode-gitman/issues"
  },
  "author": {
    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/vscode-gitman.git"
  },
  "engines": {
    "vscode": "^1.15.0"
  },
  "keywords": [
    "vscode",
    "vsc",
    "extension"
  ],
  "categories": [
    "Other"
  ],
  "dependencies": {
    "@fabiospampinato/gitman": "^1.1.0"
  },
  "devDependencies": {
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "vscode": "^1.1.37",
    "webpack": "^5.63.0",
    "webpack-cli": "^4.9.1"
  }
}
